{
  "extends": "@ygqygq2/typescript-config/react.json",
  "compilerOptions": {
    "rootDir": ".",
    "outDir": "dist",
    "moduleResolution": "bundler",
    "ignoreDeprecations": "5.0",
    "paths": {
      "@/*": ["./src/*"]
    }
  },
  "include": ["**/*.ts", "**/*.tsx", "**/*.d.ts"],
  "exclude": ["node_modules", "dist"]
}
